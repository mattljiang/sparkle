{
  "manifest_version": 2,
  "name": "Kaleidoscope",
  "version": "1.0",
  "description": "A more fun experience when you browse the internet.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ], 
  "web_accessible_resources": ["images/*", "newtab.jpg"],
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  }
}